@model EntityFramework1Ikub.Models.Brand
@{
  ViewBag.Title = "Details";
}

<h2>Details of @Model.BrandName</h2>
<div class="container">
  <ul>
    <li>
      @Html.LabelFor(m => m.BrandID):
      @Html.DisplayFor(m => m.BrandID)
    </li>
    <li>
      @Html.LabelFor(m => m.BrandName):
      @Html.DisplayFor(m => m.BrandName)
    </li>
  </ul>
</div>


